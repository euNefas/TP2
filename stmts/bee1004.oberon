MODULE bee1004;

IMPORT Input, Texts;

VAR
    A, B, produto: INTEGER;

PROCEDURE ProdutoSimples;
BEGIN
    Input.Int(A);
    Input.Int(B);

    produto := A * B;

    Texts.WriteString(Texts.Console, "PROD = ");
    Texts.WriteInt(Texts.Console, produto, 0);
    Texts.WriteLn(Texts.Console);
END ProdutoSimples;

BEGIN
    ProdutoSimples;
END bee1004.