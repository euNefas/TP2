MODULE bee1011;

IMPORT Input, Texts;

VAR
    r: INTEGER;
    volume, pi: REAL;

PROCEDURE Esfera;
BEGIN
    pi:= 3.14159;

    Input.Int(r);
    
    volume := (4/3.0)*pi*(r*r*r);

    Texts.WriteString(Texts.Console, "VOLUME = ");
    Texts.WriteReal(Texts.Console, volume, 3);
    Texts.WriteLn(Texts.Console);
END Esfera;

BEGIN
    Esfera;
END bee1011.